<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <appSettings>
    <add key="ControlIntervalSeconds" value="30"/>          <!-- Time in seconds for loop -->
    <add key="ZabbixServerAddress" value="172.28.151.30"/>  <!-- Address of Zabbix Server -->
    <add key="ZabbixServerPort" value="10051"/>             <!-- Port of Zabbix Server Trapper -->
      
    <add key="ZabbixClientHostName" value=""/>              <!-- Host Name of this equipment in Zabbix - If Empty, service uses windows AD Hostname (Without domain) -->

    <add key="DoNotSendDataToZabbix" value="false"/>         <!-- if true do not send data to zabbix, only query info and write log -->
  </appSettings>

  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="log\\log.log"/>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>                      <!-- Number of History Log files -->
      <maximumFileSize value="5MB"/>                        <!-- Log file size -->
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <!-- param name="Header" value="[Header]%newline" / -->
        <!-- param name="Footer" value="[Footer]%newline" / -->
        <!-- conversionPattern value="[%date] |%-5.5thread| %-5level | %-10.10logger |... %10.10class::%-20.30method | %message%exception%newline" / -->
        <!-- conversionPattern value="[%date] | %-5level |... %10.10class::%-30.30method | %message%exception%newline"/ -->
        <conversionPattern value="[%date] | %-5level | %message%exception%newline"/>
      </layout>
    </appender>
    <root>
      <level value="ALL"/>
      <appender-ref ref="LogFileAppender"/>
    </root>
  </log4net>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>       <!-- Do not modify -->
  </startup>
</configuration>
